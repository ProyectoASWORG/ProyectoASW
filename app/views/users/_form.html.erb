<%= form_with(model: user, url: users_update_path(@user)) do |form| %>
   
        <div class="field">
         <%= form.label :about %>
         <%= form.text_field :text %>
        </div>
      
       <div class="field">
         <%= form.label :email%>
         <%= form.text_field :user_id%>
       </div>
      
       <div class="actions">
            <%= form.submit %>
       </div>
<% end %>